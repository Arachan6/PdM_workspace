#include "stm32f4xx_hal.h"
#include "API_uart.h"

/* Function prototypes */
void SystemClock_Config(void);

int main(void){
    HAL_Init();
    SystemClock_Config();
    UART5_Init();
    USART2_Init();
    uint8_t rxData;
    while (1){
        // Receive data from UART5
        if (UART5_Receive_Char((uint8_t*) rxData) == HAL_OK)
        {
            // Send received data to USART2
        	UART5_Send_String(rxData);
        }
    }
}





void SystemClock_Config(void)
{
    // Clock configuration code (usually auto-generated by STM32CubeMX)
}

void Error_Handler(void)
{
    /* Turn on LED or debug LED to indicate error */
    while (1)
    {
        // Loop forever
    }
}



