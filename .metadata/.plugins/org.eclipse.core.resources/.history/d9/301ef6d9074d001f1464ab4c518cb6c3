#include "stm32f4xx_hal.h"
#include "API_uart.h"
#include "API_gps.h"

/* Function prototypes */
void SystemClock_Config(void);

#define DEBUG false

int main(void){
    HAL_Init();
    SystemClock_Config();
    UART5_Init();
    USART2_Init();

    while (1){
    	if (nmea_sentence_received() == true){
    		char* nmea_sentence = get_nmea_sentence();
			#if DEBUG
    			USART2_Send_String((uint8_t*)"(DEBUG) ");
				USART2_Send_String((uint8_t*)nmea_sentence);
			#endif
			Parse_NMEA_Sentence(nmea_sentence);
    	}
    }
}





void SystemClock_Config(void)
{
    // Clock configuration code (usually auto-generated by STM32CubeMX)
}

void Error_Handler(void)
{
    /* Turn on LED or debug LED to indicate error */
    while (1)
    {
        // Loop forever
    }
}



