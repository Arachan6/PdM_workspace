#include "main.h"

bool_t String_Compare(const char *str1, const char *str2) {
	// Loops until null terminator in str1 "\0"
    while (*str1 && (*str1 == *str2)) {
        str1++;
        str2++;
    }
    return *str1 == *str2;
}
